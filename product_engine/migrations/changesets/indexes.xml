<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
    xmlns:pro="http://www.liquibase.org/xml/ns/pro"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd
        http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd
        http://www.liquibase.org/xml/ns/pro http://www.liquibase.org/xml/ns/pro/liquibase-pro-latest.xsd">
    
    <changeSet author="u.goncharenko" id="create-indexes">
        <createIndex indexName="index_first_name" tableName="client">
            <column name="first_name"/>
        </createIndex>

        <createIndex indexName="index_second_name" tableName="client">
            <column name="second_name"/>
        </createIndex>

        <createIndex indexName="index_third_name" tableName="client">
            <column name="third_name"/>
        </createIndex>
       
    </changeSet>
    
</databaseChangeLog>
